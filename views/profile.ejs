    <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title"><strong>Questionnaire</strong></h3>
            </div>
            <div class="panel-body">
            	<h4><strong>Prefered Name/ Nickname:</strong></h4>
                <input type="text" name="temp" placeholder="John/Jane" id="q1">

                <h4><strong>Age:</strong></h4>
                <input type="text" name="temp" placeholder="" id="q2">

                <h4><strong>Height in Inches:</strong></h4>
                <input type="text" name="temp" placeholder="70" id="q3">

                <h4><strong>Weight in lbs:</strong></h4>
                <input type="text" name="temp" placeholder="140" id="q4">

                <h4><strong>Gender:</strong></h4>
                <input type="text" name="temp" placeholder="female" id="q5">

                <h4><strong> Are you trying to conceive?</strong></h4>
                <select data-placeholder="" class="chosen-select" id="q8">
                    <option value="1">yes</option>
                    <option value="2">no</option>
                </select>

                
            </div>
        </div>
 <button id="submitButton" type="submit" class="btn btn-primary">
